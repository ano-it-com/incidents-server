{
  "name": "ano-it-com/eav-bundle",
  "version": "0.1.0",
  "description": "Typed EAV Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "AUTONOMOUS NONPROFIT ORGANIZATION \"RESEARCH CENTER\" INFORMATION TECHNOLOGIES",
      "homepage": "https://github.com/ano-it-com/"
    }
  ],
  "require": {
    "php": "^7.4",
    "doctrine/dbal": "^2.10",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "ramsey/uuid": "^4.1",
    "symfony/framework-bundle": "^5.2",
    "symfony/yaml": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ANOITCOM\\EAVBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ANOITCOM\\EAVBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.2"
    }
  }
}
